<odoo>
    <record id="custom_sale_order_report" model="ir.actions.report">
        <field name="name">Custom Sale Order Report</field>
        <field name="model">sale.order</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">sale_order_pdf_button.custom_sale_order_report_template</field>
        <field name="report_file">sale_order_pdf_button.custom_sale_order_report_template</field>
        <field name="print_report_name">'Sales Report - %s' % (object.id)</field>
        <field name="attachment"/>
        <field name="binding_model_id" eval="False"/>
    </record>
    <record id="custom_sale_order_proposal_report_paperformate" model="report.paperformat">
        <field name="name">Custom Sale Order Proposal Report PaperFormat</field>
        <field name="default" eval="True"/>
        <field name="format">A4</field>
        <field name="orientation">Portrait</field>
        <field name="margin_top">0</field>
        <field name="margin_left">0</field>
        <field name="margin_right">0</field>
        <field name="margin_bottom">0</field>
        <field name="dpi">90</field>
    </record>

    <record id="custom_sale_order_proposal_report" model="ir.actions.report">
        <field name="name">Custom Sale Order Proposal Report</field>
        <field name="model">sale.order</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">sale_order_pdf_button.custom_sale_order_proposal_report_template</field>
        <field name="report_file">sale_order_pdf_button.custom_sale_order_proposal_report_template</field>
        <field name="print_report_name">'Sales Report Proposal- %s' % (object.id)</field>
        <field name="attachment"/>
        <field name="binding_model_id" ref="sale.model_sale_order"/>
         <field name="paperformat_id" ref="sale_order_pdf_button.custom_sale_order_proposal_report_paperformate"/>
    </record>



